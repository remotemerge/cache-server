{
  "plugins": [],
  "presets": [
    "@babel/preset-flow"
  ],
  "env": {
    "development": {
      "presets": [
        "@babel/preset-env"
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "> 1%",
                "last 2 versions",
                "not ie <= 8"
              ]
            },
            "useBuiltIns": "usage"
          }
        ]
      ]
    }
  },
  "ignore": [
    "dist/*.js"
  ]
}
