  "description": "The cache server is a DOM rendering engine built on Node, Express and Google Puppeteer.",
  "author": {
    "name": "Madan Sapkota",
    "email": "code@madansapkota.com",
    "url": "https://madansapkota.com"
  },
  "license": "MIT",
  "private": true,
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:sapkotamadan/cache-server.git"
  },
  "bugs": {
    "url": "https://github.com/sapkotamadan/cache-server/issues"
  },
  "keywords": [
    "cache-server"
  ],
  "os": [
    "darwin",
    "win32",
    "linux"
  ],
  "engines": {
    "node": ">=8.16",
    "npm": ">=6.12",
    "yarn": ">=1.17"
  },
  "node_version": "8.16",
  "scripts": {
    "test": "node src/Package.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --mode=development",
