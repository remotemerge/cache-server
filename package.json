    "puppeteer": "^1.12.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/node": "^11.9.3",
    "@types/webpack": "^4.4.24",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-loader": "^2.1.2",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.2",
    "webpack": "^4.29.3",
